{
  "name": "plenexyui",
  "author": "Roman Kapralov <qube100@yandex.ru>",
  "private": false,
  "version": "0.1.18",
  "type": "module",
  "files": [
    "./dist"
  ],
  "main": "./dist/plenexy-tailwind-ui.umd.js",
  "module": "./dist/plenexy-tailwind-ui.es.js",
  "exports": {
    ".": {
      "import": "./dist/plenexy-tailwind-ui.es.js",
      "require": "./dist/plenexy-tailwind-ui.umd.js"
    },
    "./dist/plenexy-tailwind-ui.css": {
      "import": "./dist/plenexy-tailwind-ui.css",
      "require": "./dist/plenexy-tailwind-ui.css"
    }
  },
  "types": "./dist/index.d.ts",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/plenexy/plenexyui.git"
  },
